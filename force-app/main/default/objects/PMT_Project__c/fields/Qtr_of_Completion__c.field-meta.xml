    <fullName>Qtr_of_Completion__c</fullName>
    <description>Qtr of Completion</description>
    <externalId>false</externalId>
    <formula>&quot;FY&quot; + CASE(MONTH(Deadline_formula__c),
1,TEXT(MOD(Year(Deadline_formula__c),100)),
