CASE(MONTH(Deadline_formula__c), 
1,&quot;Q4&quot;, 
2,&quot;Q1&quot;, 
3,&quot;Q1&quot;, 
4,&quot;Q1&quot;, 
5,&quot;Q2&quot;, 
6,&quot;Q2&quot;, 
7,&quot;Q2&quot;, 
8,&quot;Q3&quot;, 
9,&quot;Q3&quot;, 
10,&quot;Q3&quot;, 
11,&quot;Q4&quot;, 
12,&quot;Q4&quot;, 
&quot;&quot;)</formula>
